<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="291.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
      <TextField fx:id="usernameTF" layoutX="68.0" layoutY="67.0" promptText="Enter Username" />
      <TextField fx:id="passwordTF" layoutX="68.0" layoutY="109.0" promptText="Enter Password" />
      <Label fx:id="passwordText" layoutX="116.0" layoutY="92.0" text="Password">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <Button fx:id="loginButton" layoutX="68.0" layoutY="144.0" mnemonicParsing="false" onAction="#onLoginClicked" text="Login" />
      <Button fx:id="exitButton" layoutX="183.0" layoutY="144.0" mnemonicParsing="false" onAction="#onExitClicked" text="Exit" />
      <Label fx:id="timeZoneText" layoutX="25.0" layoutY="187.0" text="Location">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="zoneIDTF" layoutX="8.0" layoutY="231.0" prefHeight="25.0" prefWidth="101.0" />
      <TextField fx:id="dateTimeTF" layoutX="116.0" layoutY="231.0" />
      <Label layoutX="149.0" layoutY="187.0" text="Date/Time">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   <Label fx:id="userNameText" layoutX="112.0" layoutY="48.0" text="Username">
      <font>
         <Font name="System Bold" size="13.0" />
      </font>
   </Label>
</AnchorPane>
