<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
   <children>
      <TextField fx:id="usernameTF" layoutX="131.0" layoutY="131.0" promptText="Enter Username" />
      <TextField fx:id="passwordTF" layoutX="334.0" layoutY="131.0" promptText="Enter Password" />
      <Label layoutX="178.0" layoutY="81.0" text="Username">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="395.0" layoutY="81.0" text="Password">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="286.0" layoutY="38.0" text="Login">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="loginButton" layoutX="180.0" layoutY="175.0" mnemonicParsing="false" onAction="#onLoginClicked" text="Login" />
      <Button fx:id="exitButton" layoutX="396.0" layoutY="175.0" mnemonicParsing="false" onAction="#onExitClicked" text="Exit" />
      <Label layoutX="193.0" layoutY="251.0" text="TimeZone">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField layoutX="339.0" layoutY="252.0" />
   </children>
</AnchorPane>
