<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="291.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
   <children>
      <TextField fx:id="usernameTF" layoutX="68.0" layoutY="31.0" promptText="Enter Username" />
      <TextField fx:id="passwordTF" layoutX="69.0" layoutY="94.0" promptText="Enter Password" />
      <Label fx:id="userNameText" layoutX="114.0" layoutY="14.0" text="Username">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="passwordText" layoutX="116.0" layoutY="63.0" text="Password">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button fx:id="loginButton" layoutX="68.0" layoutY="144.0" mnemonicParsing="false" onAction="#onLoginClicked" text="Login" />
      <Button fx:id="exitButton" layoutX="183.0" layoutY="144.0" mnemonicParsing="false" onAction="#onExitClicked" text="Exit" />
      <Label fx:id="timeZoneText" layoutX="103.0" layoutY="187.0" text="Time Zone">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="zoneIDTF" layoutX="69.0" layoutY="232.0" />
   </children>
</AnchorPane>
